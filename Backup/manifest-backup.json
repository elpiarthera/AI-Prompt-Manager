{
  "manifest_version": 3,
  "name": "AI Prompt Manager",
  "version": "1.0",
  "description": "Manage and use AI prompts",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ]
}
